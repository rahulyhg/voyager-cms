{
    "name": "tjventurini/voyager-cms",
    "description": "Custom CMS functionality for the voyager admin panel.",
    "type": "package",
    "authors": [
        {
            "name": "Thomas Venturini",
            "email": "thomas@venturini.codes"
        }
    ],
    "require": {
        "nuwave/lighthouse": "^3.7",
        "tcg/voyager": "^1.2",
        "tjventurini/voyager-projects": "*",
        "tjventurini/voyager-content-blocks": "*",
        "tjventurini/voyager-pages": "*",
        "tjventurini/voyager-posts": "*",
        "tjventurini/voyager-users": "*",
        "laravel/passport": "^7.3",
        "joselfonseca/lighthouse-graphql-passport-auth": "^1.3"
    },
    "require-dev": {
        "mll-lab/laravel-graphql-playground": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tjventurini\\VoyagerCMS\\Seeds\\": "database/seeds/",
            "Tjventurini\\VoyagerCMS\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tjventurini\\VoyagerCMS\\VoyagerCMSServiceProvider"
            ]
        }
    }
}
